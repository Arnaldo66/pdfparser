{
    "name": "arnaldo66/pdfparser",
    "description": "Pdf parser library. Can read and extract information from pdf file. fork from smalot",
    "keywords": ["PDF", "text", "parser", "parse", "extract"],
    "type": "library",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Arnaldo66",
            "email": "arnaldo66@yahoo.fr"
        }
    ],
    "support": {
        "issues": "https://github.com/smalot/pdfparser/issues"
    },
    "homepage": "https://www.pdfparser.org",
    "require": {
        "php": ">=7.1",
        "symfony/polyfill-mbstring": "^1.18",
        "ext-zlib": "*",
        "ext-iconv": "*"
    },
    "autoload": {
        "psr-0": {
            "Arnaldo66\\PdfParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Arnaldo66\\PdfParser\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 1200
    }
}
